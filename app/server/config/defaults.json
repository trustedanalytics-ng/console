{
  "timeout": 30000,
  "uploader_config": {
    "file_size_limit": 500,
    "file_black_list_types": [
      "application/pdf",
      "application/x-msdownload"
    ]
  },
  "metrics_grafana_host": "localhost:3000",
  "session_config": {
    "idle_time": 1800,
    "keepalive_time": 5
  },
  "reverse_proxy": {
    "prefix": "/api/v1",
    "request_prefix": "/rest"
  }
}